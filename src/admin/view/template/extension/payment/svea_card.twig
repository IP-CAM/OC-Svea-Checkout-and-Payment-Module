{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
      <div class="container-fluid">
        <div class="pull-right">
          <button type="submit" onclick="$('#form').submit();" form="form-sveacard" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
          <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
        <h1>{{ heading_title }}</h1>
        <ul class="breadcrumb">
          {% for breadcrumb in breadcrumbs %}
          <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
          {% endfor %}
        </ul>
      </div>
    </div>
    <div class="container-fluid">
    {% if error_warning %}
    <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
        <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-pencil"></i> </h3>
        </div>
    <div class="panel-body">
        <!--general settings -->
        <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-sveacard" class="form-horizontal">
            <div class="form-group">
                <div class="col-sm-3 control-label">Version</div>
                <div class="col-sm-9">{{ payment_svea_version_text }}</div>
                <input type="hidden" value="{{ payment_svea_version }}" name="payment_svea_card_version" id="payment_svea_card_version" />
            </div>
              <div class="form-group">
                    <label for="payment_svea_card_testmode" class="col-sm-3 control-label">{{ entry_testmode }}</label>
                    <div class="col-sm-9">
                        <select name="payment_svea_card_testmode">
                            <option value="1" {% if payment_svea_card_testmode  ==  '1'%} {{ 'selected="selected"' }}{% endif %} >{{ text_enabled }}</option>
                            <option value="0" {% if payment_svea_card_testmode  ==  '0'%} {{ 'selected="selected"' }}{% endif %} >{{ text_disabled }}</option>
                        </select>
                    </div>
                </div>
              <div class="form-group">
                    <label for="payment_svea_geo_zone_id" class="col-sm-3 control-label">{{ entry_geo_zone }}</label>
                    <div class="col-sm-9">
                         <select name="payment_svea_geo_zone_id">
                            <option value="0">{{ text_all_zones }}</option>
                            {% for geo_zone in geo_zones %}
                                {% if geo_zone.geo_zone_id  ==  payment_svea_card_geo_zone_id %}
                            <option value="{{ geo_zone.geo_zone_id }}" selected="selected">{{ geo_zone.name }}</option>
                                 {% else %}
                            <option value="{{ geo_zone.geo_zone_id }}">{{ geo_zone.name }}</option>
                                {% endif %}
                                {% endfor %}
                      </select>
                    </div>
                </div>
              <div class="form-group">
                    <label for="payment_svea_card_status" class="col-sm-3 control-label">{{ entry_status }}</label>
                    <div class="col-sm-9">
                        <select name="payment_svea_card_status">
                            {% if payment_svea_card_status %}
                            <option value="1" selected="selected">{{ text_enabled }}</option>
                            <option value="0">{{ text_disabled }}</option>
                             {% else %}
                            <option value="1">{{ text_enabled }}</option>
                            <option value="0" selected="selected">{{ text_disabled }}</option>
                            {% endif %}
                        </select>
                    </div>
                </div>
            <div class="form-group">
                <label class="col-sm-3 control-label" for="input-process-status">{{ entry_card_logos }}</label>
                <div class="col-sm-9">
                    <div class="well well-sm" style="height: 120px; overflow: auto;">
                        {% for card_name, card_logo in card_logos %}
                        <div class="checkbox">
                            <label>
                                {% if card_name in payment_svea_card_logos %}
                                <input type="checkbox" name="payment_svea_card_logos[]" value="{{ card_name}}" checked="checked" />
                                <img src="{{ card_logo }}" />
                                {% else %}
                                <input type="checkbox" name="payment_svea_card_logos[]" value="{{ card_name }}" />
                                <img src="{{ card_logo }}" />
                                {% endif %}
                            </label>
                        </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
              <div class="form-group">
                    <label for="payment_svea_card_sort_order" class="col-sm-3 control-label">{{ entry_sort_order }}</label>
                    <div class="col-sm-9">
                       <input type="text" name="payment_svea_card_sort_order" value="{{ payment_svea_card_sort_order }}" size="1" />
                    </div>
                </div>
              <div class="form-group">
                    <label for="payment_svea_card_payment_svea_description" class="col-sm-3 control-label">{{ entry_payment_svea_description }}</label>
                    <div class="col-sm-9">
                      <textarea rows="2" cols="30" name="payment_svea_card_payment_svea_description">{{ payment_svea_card_payment_svea_description }}</textarea>
                    </div>
                </div>




                <!-- order statuses -->
                <div class="form-group">
                    <h5 class="col-sm-3 control-label">{{ entry_order_status }}</h5>
                    <div class="col-sm-9"></div>
                </div>
                <div class="form-group">
                    <label for="payment_svea_card_order_status_id" class="col-sm-3 control-label">
                       {{ entry_status_order }}
                    </label>
                     <div class="col-sm-9">
                          <select name="payment_svea_card_order_status_id">
                            {% for order_status in order_statuses %}
                            {% if order_status.order_status_id  ==  payment_svea_card_order_status_id %}
                            <option value="{{ order_status.order_status_id }}" selected="selected">{{ order_status.name }}</option>
                            {% else %}
                            <option value="{{ order_status.order_status_id }}">{{ order_status.name }}</option>
                            {% endif %}
                            {% endfor %}
                        </select>
                     </div>
                </div>
              <div class="form-group">
                  <label for="payment_svea_card_canceled_status_id" class="col-sm-3 control-label">
                        <span data-toggle="tooltip" title="{{ entry_status_canceled_text }}">{{ entry_status_canceled }}</span>
                    </label>
                     <div class="col-sm-9">
                         <select name="payment_svea_card_canceled_status_id">
                            {% for order_status in order_statuses %}
                            {% if order_status.order_status_id  ==  payment_svea_card_canceled_status_id %}
                             <option value="{{ order_status.order_status_id }}" selected="selected">{{ order_status.name }}</option>
                             {% else %}
                             <option value="{{ order_status.order_status_id }}">{{ order_status.name }}</option>
                            {% endif %}
                            {% endfor %}
                        </select>
                     </div>
                </div>
                 <div class="form-group">
                  <label for="payment_svea_card_deliver_status_id" class="col-sm-3 control-label">
                        <span data-toggle="tooltip" title="{{ entry_status_confirmed_text }}">{{ entry_status_confirmed }}</span>
                    </label>
                     <div class="col-sm-9">
                        <select name="payment_svea_card_deliver_status_id">
                        {% for order_status in order_statuses %}
                            {% if order_status.order_status_id  ==  payment_svea_card_deliver_status_id %}
                            <option value="{{ order_status.order_status_id }}" selected="selected">{{ order_status.name }}</option>
                             {% else %}
                            <option value="{{ order_status.order_status_id }}">{{ order_status.name }}</option>
                            {% endif %}
                            {% endfor %}
                        </select>
                     </div>
                </div>
              <div class="form-group">
                    <label for="payment_svea_card_refunded_status_id" class="col-sm-3 control-label">
                        <span data-toggle="tooltip" title="{{ entry_status_refunded_text }}">{{ entry_status_refunded }}</span>
                    </label>
                     <div class="col-sm-9">
                        <select name="payment_svea_card_refunded_status_id">
                            {% for order_status in order_statuses %}
                                {% if order_status.order_status_id  ==  payment_svea_card_refunded_status_id %}
                             <option value="{{ order_status.order_status_id }}" selected="selected">{{ order_status.name }}</option>
                                {% else %}
                             <option value="{{ order_status.order_status_id }}">{{ order_status.name }}</option>
                                {% endif %}
                            {% endfor %}
                        </select>
                     </div>
                </div>

        <!-- Mode specific -->
        <div class="tab-content">
            <ul class="nav nav-tabs" id="payment_svea_merchant">
                <li><a href="#tab-cardtest" data-toggle="tab">Test</a></li>
                <li><a href="#tab-cardprod" data-toggle="tab">Prod</a></li>
            </ul>
            <div class="tab-content">
                  <!--Test -->
                <div class="tab-pane" id="tab-cardtest">
                     <div class="form-group">
                        <label class="col-sm-3 control-label" for="payment_svea_card_merchant_id_test">{{ entry_merchant_id }}</label>
                        <div class="col-sm-9">
                            <input name="payment_svea_card_merchant_id_test" type="text"
                                    value="{{ value_merchant_test }}" />
                        </div>
                    </div>
                     <div class="form-group">
                        <label class="col-sm-3 control-label" for="payment_svea_card_sw_test">{{ entry_sw }}</label>
                        <div class="col-sm-9">
                            <input name="payment_svea_card_sw_test" type="text"
                                    value="{{ value_sw_test }}" />
                        </div>
                    </div>
                </div>
                <!--Prod -->
                <div class="tab-pane" id="tab-cardprod">
                     <div class="form-group">
                        <label class="col-sm-3 control-label" for="payment_svea_card_merchant_id_prod">{{ entry_merchant_id }}</label>
                        <div class="col-sm-9">
                            <input name="payment_svea_card_merchant_id_prod" type="text"
                                    value="{{ value_merchant_prod }}" />
                        </div>
                    </div>
                     <div class="form-group">
                        <label class="col-sm-3 control-label" for="payment_svea_card_sw_prod">{{ entry_sw }}</label>
                        <div class="col-sm-9">
                            <input name="payment_svea_card_sw_prod" type="text"
                                    value="{{ value_sw_prod }}" />
                        </div>
                    </div>
                </div>
            </div>
        </div>


      </form>
    </div>
    </div><!-- panel-default -->
  </div><!-- container-fluid -->
<div style="height:100px"></div>
  <script type="text/javascript"><!--
$('#payment_svea_merchant a:first').tab('show');
//--></script>
{{ footer }}